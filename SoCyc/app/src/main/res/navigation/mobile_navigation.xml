<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_dashboard">

    <fragment
        android:id="@+id/nav_dashboard"
        android:name="com.dubailizards.so_cyc.boundary.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        app:popUpToInclusive="true"
        />

    <fragment
        android:id="@+id/nav_navigation"
        android:name="com.dubailizards.so_cyc.boundary.navigation.NavigationFragment"
        android:label="@string/title_navigation"
        tools:layout="@layout/fragment_navigation"
        />

    <fragment
        android:id="@+id/nav_profile"
        android:name="com.dubailizards.so_cyc.boundary.profile.ProfileFragment"
        android:label="@string/title_profile"
        app:popUpToInclusive="true"
        />

</navigation>